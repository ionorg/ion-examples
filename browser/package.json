{
  "name": "ta1k",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=13.0.0",
    "yarn": ">= 1.19.0"
  },
  "scripts": {
    "build": "./cli.js build",
    "icons": "vsvg -s theme/svg/ --es6 -t theme/icons",
    "lint": "yarn lint:js;yarn lint:scss",
    "lint:js": "eslint \"{sip,sig11,theme,webphone}/**/{*.js,*.vue}\"",
    "lint:scss": "stylelint 'webphone/**/*.scss'",
    "prepare": "yarn icons;yarn snowpack",
    "release": "standard-version",
    "snowpack": "snowpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint",
    "**/*.scss": "stylelint"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "7.x.x",
    "@garage11/vue-eslint-parser": "^8.0.0",
    "@garage11/vuepack": "1.3.0",
    "archy": "^1.0.0",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "clean-css": "^4.2.3",
    "connect": "^3.7.0",
    "connect-mount": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "7.x.x",
    "eslint-plugin-sort-class-members": "1.x.x",
    "eslint-plugin-vue": "^6.2.1",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "node-sass-glob-importer": "^5.3.2",
    "rc": "^1.2.8",
    "serve-static": "^1.14.1",
    "snowpack": "2.x.x",
    "stylelint": "13.x.x",
    "stylelint-order": "4.x.x",
    "stylelint-scss": "3.x.x",
    "tildify": "^2.0.0",
    "tiny-lr": "^1.1.1",
    "yargs": "^15.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "sdp-transform": "^2.14.0",
    "vue": "2.6.11",
    "vue-stash": "^2.0.1-beta"
  }
}
